{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "types": [
      "node"
    ],
    "lib": [
      "es5"
    ],
    "outDir": "out",
    "declarationDir": "."
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": ["out"]
}
